@Article{Abdel-Khalik-2016-p1331-1343,
  Title                    = {Steady-State Mathematical Modeling of a Five-Phase Induction Machine With a Combined Star/Pentagon Stator Winding Connection},
  Author                   = {A. S. Abdel-Khalik and S. Ahmed and A. M. Massoud},
