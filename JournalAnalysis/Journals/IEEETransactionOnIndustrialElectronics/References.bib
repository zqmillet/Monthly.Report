@Article{Nallamekala-2016-p1450-1457,
  Title                    = {A Fault-Tolerant Dual Three-Level Inverter Configuration for Multipole Induction Motor Drive With Reduced Torque Ripple},
  Author                   = {K. K. Nallamekala and K. Sivakumar},
  Journal                  = {IEEE Transactions on Industrial Electronics},
  Year                     = {2016},

  Month                    = {March},
  Number                   = {3},
  Pages                    = {1450-1457},
  Volume                   = {63},

  Abstract                 = {Multilevel inverters are gaining more attention in ac drive application due to their many attractive features. In the case of conventional neutral-point-clamped (NPC) or flying capacitor multilevel inverter configurations, active switches are connected in series to produce multilevel output voltage waveform. Therefore, if any one switch fails, the entire configuration has to be shut down; this will reduce the reliability of the system. A dual three-level inverter configuration for induction motor drive is proposed in this paper to improve reliability of the system. This topology is developed by feeding four-pole induction motor stator winding with four conventional two-level inverter modules. A level-shifted carrier-based third harmonic injection pulsewidth-modulation technique is used to produce the gating signals for the proposed configuration. By providing proper phase shift between carrier waves, multilevel voltage waveform is produced across the total motor phase winding, and first center band harmonics are also canceled. Thereby, the torque ripple will be considerably reduced compared with conventional NPC five-level inverter-driven induction motor drive. Finite-element analysis (FEA) is used to estimate the torque ripple when induction motor is supplied by the proposed configuration and conventional five-level NPC inverter configuration to show the effectiveness of the proposed converter. The proposed configuration is simulated using MATLAB/Simulink and experimentally verified using a laboratory prototype with a 5-hp four-pole induction motor drive.},
  Doi                      = {10.1109/TIE.2015.2495281},
  File                     = {Nallamekala-2016-p1450-1457.pdf:References\\Nallamekala-2016-p1450-1457.pdf:PDF},
  ISSN                     = {0278-0046},
  Keywords                 = {PWM invertors;fault tolerance;finite element analysis;induction motor drives;machine windings;power capacitors;power conversion harmonics;switching convertors;AC drive application;FEA;MATLAB-Simulink;NPC multilevel inverters;active switches;carrier waves;center band harmonics;fault-tolerant dual three-level inverter configuration;finite element analysis;flying capacitor multilevel inverter;level-shifted carrier-based third harmonic injection technique;motor phase winding;multipole induction motor drive;neutral-point-clamped multilevel inverters;phase shift;pulse width-modulation technique;reduced torque ripple;two-level inverter modules;Harmonic analysis;Induction motors;Inverters;Modulation;Switches;Topology;Windings;Multi pole pair induction motor drive;Multi-level inverter;Multilevel inverter;Relaibility issues;Third harmonic injection PWM;Torque ripple;multi-pole-pair induction motor drive;reliability issues;third harmonic injection pulsewidth modulation (PWM);torque ripple},
  Reviewtime               = {109},
  Url                      = {http://ieeexplore.ieee.org/xpl/articleDetails.jsp?arnumber=7308060}
}

@Article{Nasiri-Zarandi-2016-p1684-1693,
